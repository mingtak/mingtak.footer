<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="mingtak.footer">

    <configlet
        title="Footer setup"
        action_id="mingtak.footer.footer"
        appId="mingtak.footer"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@footerSettingControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
